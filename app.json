{
  "expo": {
    "name": "Fitchain App",
    "slug": "fitchain-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/Logo-Icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/Logo.png",
      "resizeMode": "native",
      "backgroundColor": "black"
    },
    "backgroundColor": "#2e2d36",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/Logo-Icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
